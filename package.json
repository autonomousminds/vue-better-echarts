{
  "name": "@autonomousminds-public/vue-better-bi",
  "version": "0.1.7",
  "description": "Vue 3 charting library built on ECharts with advanced formatting and theming",
  "type": "module",
  "main": "./dist/vue-better-bi.umd.cjs",
  "module": "./dist/vue-better-bi.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-better-bi.js",
      "require": "./dist/vue-better-bi.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix --ignore-path .gitignore",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "chroma-js": "^2.4.2",
    "debounce": "^2.0.0",
    "downloadjs": "^1.4.7",
    "echarts": "^5.5.0",
    "echarts-stat": "^1.2.0",
    "export-to-csv": "^1.3.0",
    "html-to-image": "^1.11.13",
    "leaflet": "^1.9.4",
    "ssf": "^0.11.2"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.4.4",
    "@types/debounce": "^1.2.4",
    "@types/downloadjs": "^1.4.6",
    "@types/leaflet": "^1.9.12",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/tsconfig": "^0.5.1",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.2.0",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "echarts",
    "charts",
    "visualization",
    "data-visualization",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
